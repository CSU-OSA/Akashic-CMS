{
  "kind": "collectionType",
  "collectionName": "resources",
  "info": {
    "singularName": "resource",
    "pluralName": "resources",
    "displayName": "Resource",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "richtext"
    },
    "media": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "name": {
      "type": "string"
    },
    "playCount": {
      "type": "biginteger",
      "default": "0"
    },
    "likeCount": {
      "type": "biginteger"
    },
    "collectedCount": {
      "type": "biginteger"
    },
    "viewCount": {
      "type": "biginteger"
    },
    "uploadDate": {
      "type": "datetime"
    },
    "readable_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "readable_resources"
    },
    "owners": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "own_resources"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "censoring",
        "published",
        "blocked"
      ],
      "private": true,
      "default": "censoring",
      "required": true
    }
  }
}
